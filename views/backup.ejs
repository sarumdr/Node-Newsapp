<div class="container">
        <h1 class=' '>  News</h1>
  <div class="row news_card">
        <div key={index} class='col-lg-4 col-md-3 col-sm-6 news container'>
        <h2> <%=post%></h2>
        <% post.forEach(function(article,index){%>
                <img src="<%=article.urlToImage%>"class="image"/>:
            
              <h3 class=' title'><%=article.title%></h3>
            <p class=' desc'><%=article.description%></p>
            <span class=' published'><%=article.publishedAt%><%=article.author%></span>
            <%})%>
        </div>

        </div>
  </div>
